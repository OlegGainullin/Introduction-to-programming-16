#include <stdio.h>
#include <locale.h>
#include <stdlib.h>
#include <math.h>

int main ()
{
    char *locale = setlocale(LC_ALL, "");
    int n = 0;
    int answer = 0;
    printf("Введите N: ");
    scanf("%d", &n);
    int a[n];
    for (int i = 0; i<n; i++)
    {
        a[i] = i*2+1;
    }
    for (int i = 0; i<n; i++)
    {
        printf("%d ", a[i]);
    }
    return 0;
}
